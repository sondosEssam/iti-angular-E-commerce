{
  "version": 2,
  "buildCommand": "cd E-commerce && npm install && npm run build",
  "outputDirectory": "E-commerce/dist/E-commerce/browser",
  "framework": "angular",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/server.js" },
    { "src": "/(.*)", "dest": "/E-commerce/dist/E-commerce/browser/index.html" }
  ]
}