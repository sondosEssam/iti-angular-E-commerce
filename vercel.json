{
  "version": 2,
  "builds": [
    {
      "src": "E-commerce/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "E-commerce/dist/browser/E-commerce"
      }
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/server.js" },
    { "src": "/(.*)", "dest": "/E-commerce/dist/E-commerce/browser/index.html" }
  ]
}